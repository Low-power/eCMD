#!/usr/bin/perl
# This wrapper can be removed when create_links is removed from makefiles

my $osname = `uname -a`;


if ($osname =~ /AIX/) {
    system("ecmdVersion_aix @ARGV");
} elsif ($osname =~ /ppc/) {
    system("ecmdVersion_ppc @ARGV");
} else {
    system("ecmdVersion_x86 @ARGV");
}

exit($?/256);  #return code from the system call above

